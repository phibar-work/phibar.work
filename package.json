{
  "name": "phibar-work",
  "version": "1.7.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "changeset": "changeset",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:unit && pnpm run test:storybook",
    "test:unit": "cross-env NODE_OPTIONS=--no-deprecation vitest run --project unit",
    "test:storybook": "cross-env NODE_OPTIONS=--no-deprecation vitest run --project storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.975.0",
    "@payloadcms/admin-bar": "3.73.0",
    "@payloadcms/db-mongodb": "3.73.0",
    "@payloadcms/live-preview-react": "3.73.0",
    "@payloadcms/next": "3.73.0",
    "@payloadcms/plugin-cloud-storage": "^3.73.0",
    "@payloadcms/plugin-form-builder": "3.73.0",
    "@payloadcms/plugin-nested-docs": "3.73.0",
    "@payloadcms/plugin-redirects": "3.73.0",
    "@payloadcms/plugin-search": "3.73.0",
    "@payloadcms/plugin-seo": "3.73.0",
    "@payloadcms/richtext-lexical": "3.73.0",
    "@payloadcms/ui": "3.73.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^10.1.0",
    "dotenv": "17.2.3",
    "graphql": "^16.8.2",
    "lucide-react": "^0.563.0",
    "next": "16.1.4",
    "next-sitemap": "^4.2.3",
    "payload": "3.73.0",
    "prism-react-renderer": "^2.3.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.71.1",
    "sharp": "0.34.5",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.12",
    "@changesets/cli": "^2.29.8",
    "@playwright/test": "1.58.0",
    "@storybook/addon-a11y": "^10.2.1",
    "@storybook/addon-docs": "^10.2.1",
    "@storybook/addon-vitest": "^10.2.1",
    "@storybook/nextjs-vite": "^10.2.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "0.5.16",
    "@testing-library/react": "16.3.2",
    "@types/escape-html": "^1.0.2",
    "@types/node": "25.0.10",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/browser-playwright": "4.0.18",
    "@vitest/coverage-v8": "4.0.18",
    "copyfiles": "^2.4.1",
    "jsdom": "27.4.0",
    "playwright": "1.58.0",
    "playwright-core": "1.58.0",
    "postcss": "^8.4.38",
    "storybook": "^10.2.1",
    "tailwindcss": "^4.1.18",
    "typescript": "5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "6.0.5",
    "vitest": "4.0.18"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
